{
    "name":"node-api",
    "version":2,
    "builds": [{ "src": "index.js", "use": "@now/node" }],
    "routes": [{ "src":"/{./*}", "dest":"/index.js"}],
    "env":{
        "DB_USER": "@api-db-user",
        "DB_PASSWORD":"@api-db-password",
        "DB_HOST":"@api-db-host",
        "DB_NAME": "@api-db-name"
    }
}